<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dapur.DapurActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycleview_dapur"
        android:layout_width="0dp"
        android:layout_height="650dp"
        app:layout_constraintBottom_toTopOf="@+id/judul_main_kecil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/item_list_dapur" />

    <TextView
        android:id="@+id/judul_main_kecil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/akaya_telivigala"
        android:text="keran's coffe and space"
        android:textSize="10dp"
        android:textStyle="bold"
        android:textColor="#0f3c4c"
        android:layout_marginBottom="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>